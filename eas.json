{
  "cli": {
    "version": ">= 3.13.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "env": {
          "EXPO_NO_NEW_ARCHITECTURE": "true"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "env": {
          "EXPO_NO_NEW_ARCHITECTURE": "true"
        }
      }
    },
    "production": {
      "ios": {
        "buildConfiguration": "Release",
        "autoIncrement": true,
        "resourceClass": "m-medium",
        "env": {
          "EXPO_NO_NEW_ARCHITECTURE": "true"
        }
      }
    }
  }
}
