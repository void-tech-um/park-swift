{
  "expo": {
    "name": "Park Swift",
    "slug": "Park-Swift",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",      
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleMaps": {
          "apiKey": "AIzaSyC5Fz0BOBAJfvvMwmGB27hJYRhFNq7ll5w"
        }
    },
    "android": {
      "package": "com.voidtech.parkswift",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC5Fz0BOBAJfvvMwmGB27hJYRhFNq7ll5w"
        }
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "permissions": "The app accesses your camera and photo library to upload images."
        }
      ]
    ],
    "privacy": "public"
  }
}